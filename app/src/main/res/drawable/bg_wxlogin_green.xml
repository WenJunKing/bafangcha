<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#ff61a512" />
            <stroke android:width="1.0dip" android:color="#ff61a512" />
            <corners android:radius="5.0dip" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:startColor="#ff61a512" android:endColor="#ff61a512" android:angle="270.0" />
            <stroke android:width="1.0dip" android:color="#ff61a512" />
            <corners android:radius="5.0dip" />
        </shape>
    </item>
</selector>