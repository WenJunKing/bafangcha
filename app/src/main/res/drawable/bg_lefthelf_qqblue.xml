<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#ff1f85d7" />
            <stroke android:width="1.0dip" android:color="#ff1f85d7" />
            <corners android:radius="5.0dip" android:topRightRadius="0.0dip" android:bottomRightRadius="0.0dip" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:startColor="#ff1f85d7" android:endColor="#ff1f85d7" android:angle="270.0" />
            <stroke android:width="1.0dip" android:color="#ff1f85d7" />
            <corners android:radius="5.0dip" android:topRightRadius="0.0dip" android:bottomRightRadius="0.0dip" />
        </shape>
    </item>
</selector>